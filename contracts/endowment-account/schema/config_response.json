{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ConfigResponse",
  "type": "object",
  "required": [
    "cw20_approved_coins",
    "mgmnt_fee",
    "payout_rate"
  ],
  "properties": {
    "cw20_approved_coins": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "mgmnt_fee": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    },
    "payout_rate": {
      "type": "integer",
      "format": "uint32",
      "minimum": 0.0
    }
  }
}
