{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CreateEndowmentMsg",
  "type": "object",
  "required": [
    "beneficiary",
    "description",
    "name",
    "owner",
    "withdraw_before_maturity"
  ],
  "properties": {
    "beneficiary": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "maturity_height": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint64",
      "minimum": 0.0
    },
    "maturity_time": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint64",
      "minimum": 0.0
    },
    "name": {
      "type": "string"
    },
    "owner": {
      "type": "string"
    },
    "withdraw_before_maturity": {
      "type": "boolean"
    }
  }
}
