{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "CreateEndowmentMsg",
  "type": "object",
  "required": [
    "description",
    "endowment_beneficiary",
    "endowment_owner",
    "name",
    "withdraw_before_maturity"
  ],
  "properties": {
    "description": {
      "type": "string"
    },
    "endowment_beneficiary": {
      "type": "string"
    },
    "endowment_owner": {
      "type": "string"
    },
    "maturity_height": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint64",
      "minimum": 0.0
    },
    "maturity_time": {
      "type": [
        "integer",
        "null"
      ],
      "format": "uint64",
      "minimum": 0.0
    },
    "name": {
      "type": "string"
    },
    "split_to_liquid": {
      "anyOf": [
        {
          "$ref": "#/definitions/SplitDetails"
        },
        {
          "type": "null"
        }
      ]
    },
    "withdraw_before_maturity": {
      "type": "boolean"
    }
  },
  "definitions": {
    "Decimal": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal(1_000_000_000_000_000_000) == 1.0\n\nThe greatest possible value that can be represented is 340282366920938463463.374607431768211455 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "SplitDetails": {
      "type": "object",
      "required": [
        "default",
        "max",
        "min"
      ],
      "properties": {
        "default": {
          "$ref": "#/definitions/Decimal"
        },
        "max": {
          "$ref": "#/definitions/Decimal"
        },
        "min": {
          "$ref": "#/definitions/Decimal"
        }
      }
    }
  }
}
