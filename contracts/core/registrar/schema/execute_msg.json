{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecuteMsg",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "vault_add"
      ],
      "properties": {
        "vault_add": {
          "type": "object",
          "required": [
            "vault_addr",
            "vault_description",
            "vault_name"
          ],
          "properties": {
            "vault_addr": {
              "type": "string"
            },
            "vault_description": {
              "type": "string"
            },
            "vault_name": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "vault_update_status"
      ],
      "properties": {
        "vault_update_status": {
          "type": "object",
          "required": [
            "approved",
            "vault_addr"
          ],
          "properties": {
            "approved": {
              "type": "boolean"
            },
            "vault_addr": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "vault_remove"
      ],
      "properties": {
        "vault_remove": {
          "type": "object",
          "required": [
            "vault_addr"
          ],
          "properties": {
            "vault_addr": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "update_config"
      ],
      "properties": {
        "update_config": {
          "$ref": "#/definitions/UpdateConfigMsg"
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "update_endowment_status"
      ],
      "properties": {
        "update_endowment_status": {
          "$ref": "#/definitions/UpdateEndowmentStatusMsg"
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "update_owner"
      ],
      "properties": {
        "update_owner": {
          "type": "object",
          "required": [
            "new_owner"
          ],
          "properties": {
            "new_owner": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "create_endowment"
      ],
      "properties": {
        "create_endowment": {
          "$ref": "#/definitions/CreateEndowmentMsg"
        }
      },
      "additionalProperties": false
    }
  ],
  "definitions": {
    "CreateEndowmentMsg": {
      "type": "object",
      "required": [
        "description",
        "endowment_beneficiary",
        "endowment_owner",
        "name",
        "withdraw_before_maturity"
      ],
      "properties": {
        "description": {
          "type": "string"
        },
        "endowment_beneficiary": {
          "type": "string"
        },
        "endowment_owner": {
          "type": "string"
        },
        "maturity_height": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0.0
        },
        "maturity_time": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0.0
        },
        "name": {
          "type": "string"
        },
        "split_to_liquid": {
          "anyOf": [
            {
              "$ref": "#/definitions/SplitDetails"
            },
            {
              "type": "null"
            }
          ]
        },
        "withdraw_before_maturity": {
          "type": "boolean"
        }
      }
    },
    "Decimal": {
      "description": "A fixed-point decimal value with 18 fractional digits, i.e. Decimal(1_000_000_000_000_000_000) == 1.0\n\nThe greatest possible value that can be represented is 340282366920938463463.374607431768211455 (which is (2^128 - 1) / 10^18)",
      "type": "string"
    },
    "EndowmentStatus": {
      "type": "string",
      "enum": [
        "Inactive",
        "Approved",
        "Frozen",
        "Closed"
      ]
    },
    "SplitDetails": {
      "type": "object",
      "required": [
        "default",
        "max",
        "min"
      ],
      "properties": {
        "default": {
          "$ref": "#/definitions/Decimal"
        },
        "max": {
          "$ref": "#/definitions/Decimal"
        },
        "min": {
          "$ref": "#/definitions/Decimal"
        }
      }
    },
    "UpdateConfigMsg": {
      "type": "object",
      "required": [
        "index_fund_contract"
      ],
      "properties": {
        "accounts_code_id": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0.0
        },
        "approved_coins": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string"
          }
        },
        "index_fund_contract": {
          "type": "string"
        }
      }
    },
    "UpdateEndowmentStatusMsg": {
      "type": "object",
      "required": [
        "endowment_addr",
        "status"
      ],
      "properties": {
        "endowment_addr": {
          "type": "string"
        },
        "status": {
          "$ref": "#/definitions/EndowmentStatus"
        }
      }
    }
  }
}
