{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecuteMsg",
  "anyOf": [
    {
      "type": "object",
      "required": [
        "vault_add"
      ],
      "properties": {
        "vault_add": {
          "type": "object",
          "required": [
            "vault_addr"
          ],
          "properties": {
            "vault_addr": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "vault_update_status"
      ],
      "properties": {
        "vault_update_status": {
          "type": "object",
          "required": [
            "approved",
            "vault_addr"
          ],
          "properties": {
            "approved": {
              "type": "boolean"
            },
            "vault_addr": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "vault_remove"
      ],
      "properties": {
        "vault_remove": {
          "type": "object",
          "required": [
            "vault_addr"
          ],
          "properties": {
            "vault_addr": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "update_config"
      ],
      "properties": {
        "update_config": {
          "$ref": "#/definitions/UpdateConfigMsg"
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "update_endowment_status"
      ],
      "properties": {
        "update_endowment_status": {
          "$ref": "#/definitions/UpdateEndowmentStatusMsg"
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "update_owner"
      ],
      "properties": {
        "update_owner": {
          "type": "object",
          "required": [
            "new_owner"
          ],
          "properties": {
            "new_owner": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "create_endowment"
      ],
      "properties": {
        "create_endowment": {
          "$ref": "#/definitions/CreateEndowmentMsg"
        }
      },
      "additionalProperties": false
    }
  ],
  "definitions": {
    "CreateEndowmentMsg": {
      "type": "object",
      "required": [
        "deposit_approved",
        "endowment_beneficiary",
        "endowment_owner",
        "withdraw_approved",
        "withdraw_before_maturity"
      ],
      "properties": {
        "deposit_approved": {
          "type": "boolean"
        },
        "endowment_beneficiary": {
          "type": "string"
        },
        "endowment_owner": {
          "type": "string"
        },
        "maturity_height": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0.0
        },
        "maturity_time": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0.0
        },
        "withdraw_approved": {
          "type": "boolean"
        },
        "withdraw_before_maturity": {
          "type": "boolean"
        }
      }
    },
    "EndowmentStatus": {
      "type": "string",
      "enum": [
        "Inactive",
        "Approved",
        "Frozen",
        "Closed"
      ]
    },
    "UpdateConfigMsg": {
      "type": "object",
      "required": [
        "index_fund_contract"
      ],
      "properties": {
        "accounts_code_id": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0.0
        },
        "approved_coins": {
          "type": [
            "array",
            "null"
          ],
          "items": {
            "type": "string"
          }
        },
        "index_fund_contract": {
          "type": "string"
        }
      }
    },
    "UpdateEndowmentStatusMsg": {
      "type": "object",
      "required": [
        "address",
        "status"
      ],
      "properties": {
        "address": {
          "type": "string"
        },
        "status": {
          "$ref": "#/definitions/EndowmentStatus"
        }
      }
    }
  }
}
