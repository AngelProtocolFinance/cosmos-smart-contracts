        "description": {
          "type": "string"
        },
        "guardians_multisig_addr": {
          "type": [
            "string",
            "null"
          ]
        },
        "maturity_height": {
          "type": [
            "integer",
            "null"
          ],
          "format": "uint64",
          "minimum": 0.0
        },
        "maturity_time": {
          "type": [
            "integer",
            "null"
          ],
