{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "QueryMsg",
  "oneOf": [
    {
      "type": "object",
      "required": [
        "vault"
      ],
      "properties": {
        "vault": {
          "type": "object",
          "required": [
            "vault_addr"
          ],
          "properties": {
            "vault_addr": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "vault_list"
      ],
      "properties": {
        "vault_list": {
          "type": "object",
          "properties": {
            "acct_type": {
              "anyOf": [
                {
                  "$ref": "#/definitions/AccountType"
                },
                {
                  "type": "null"
                }
              ]
            },
            "approved": {
              "type": [
                "boolean",
                "null"
              ]
            },
            "endowment_type": {
              "anyOf": [
                {
                  "$ref": "#/definitions/EndowmentType"
                },
                {
                  "type": "null"
                }
              ]
            },
            "limit": {
              "type": [
                "integer",
                "null"
              ],
              "format": "uint64",
              "minimum": 0.0
            },
            "network": {
              "type": [
                "string",
                "null"
              ]
            },
            "start_after": {
              "type": [
                "string",
                "null"
              ]
            },
            "vault_type": {
              "anyOf": [
                {
                  "$ref": "#/definitions/VaultType"
                },
                {
                  "type": "null"
                }
              ]
            }
          }
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "config"
      ],
      "properties": {
        "config": {
          "type": "object"
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "fees"
      ],
      "properties": {
        "fees": {
          "type": "object"
        }
      },
      "additionalProperties": false
    },
    {
      "type": "object",
      "required": [
        "network_connection"
      ],
      "properties": {
        "network_connection": {
          "type": "object",
          "required": [
            "chain_id"
          ],
          "properties": {
            "chain_id": {
              "type": "string"
            }
          }
        }
      },
      "additionalProperties": false
    }
  ],
  "definitions": {
    "AccountType": {
      "type": "string",
      "enum": [
        "locked",
        "liquid"
      ]
    },
    "EndowmentType": {
      "type": "string",
      "enum": [
        "Charity",
        "Normal"
      ]
    },
    "VaultType": {
      "oneOf": [
        {
          "type": "string",
          "enum": [
            "native",
            "evm"
          ]
        },
        {
          "type": "object",
          "required": [
            "ibc"
          ],
          "properties": {
            "ibc": {
              "type": "object",
              "required": [
                "ica"
              ],
              "properties": {
                "ica": {
                  "type": "string"
                }
              }
            }
          },
          "additionalProperties": false
        }
      ]
    }
  }
}
