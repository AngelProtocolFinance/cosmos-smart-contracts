{
  "name": "",
  "description": "Typescript test suite for all integration (and day-to-day management support) platform for Angel Protocol's CosmWasm smart contracts.",
  "version": "2.1.0",
  "license": "GPL-3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/AngelProtocolFinance/cosmwasm-smart-contracts.git"
  },
  "eslintConfig": {
    "extends": [
      "prettier"
    ],
    "plugins": [
      "prettier"
    ],
    "rules": {
      "prettier/prettier": "error",
      "no-unused-vars": 0,
      "@typescript-eslint/no-unused-vars": 0,
      "@typescript-eslint/no-explicit-any": 0
    }
  },
  "scripts": {
    "test": "ts-node ./src/index.ts",
    "test:local-setup-ibc": "npm run test --mode=local_setup_ibc",
    "test:local-test-ibc": "npm run test --mode=local_test_ibc",
    "test:localterra-tests": "npm run test --mode=localterra_tests",
    "test:localterra-setup-astrovaults": "npm run test --mode=localterra_setup_astrovaults",
    "test:localterra-setup-astroport": "npm run test --mode=localterra_setup_astroport",
    "test:localjuno-tests": "npm run test --mode=localjuno_tests",
    "test:localjuno-setup-core": "npm run test --mode=localjuno_setup_core",
    "test:localjuno-setup-endowments": "npm run test --mode=localjuno_setup_endowments",
    "test:localjuno-setup-giftcards": "npm run test --mode=localjuno_setup_giftcards",
