    "test:localjuno-setup-loopvaults": "npm run test --mode=localjuno_setup_loopvaults",
    "test:localjuno-setup-loopswap": "npm run test --mode=localjuno_setup_loopswap",
    "test:localjuno-setup-junoswap": "npm run test --mode=localjuno_setup_junoswap",
    "test:localjuno-setup-halo": "npm run test --mode=localjuno_setup_halo",
    "test:localjuno-migrate-core": "npm run test --mode=localjuno_migrate_core",
    "test:localjuno-migrate-halo": "npm run test --mode=localjuno_migrate_halo",
    "test:testnet-tests": "npm run test --mode=testnet_tests",
    "test:testnet-setup-core": "npm run test --mode=testnet_setup_core",
    "test:testnet-setup-endowments": "npm run test --mode=testnet_setup_endowments",
    "test:testnet-setup-giftcards": "npm run test --mode=testnet_setup_giftcards",
    "test:testnet-setup-mockvaults": "npm run test --mode=testnet_setup_mockvaults",
    "test:testnet-setup-loopvaults": "npm run test --mode=testnet_setup_loopvaults",
    "test:testnet-setup-loopswap": "npm run test --mode=testnet_setup_loopswap",
    "test:testnet-setup-junoswap": "npm run test --mode=testnet_setup_junoswap",
    "test:testnet-setup-halo": "npm run test --mode=testnet_setup_halo",
    "test:testnet-migrate-core": "npm run test --mode=testnet_migrate_core",
    "test:testnet-migrate-halo": "npm run test --mode=testnet_migrate_halo",
    "test:mainnet-tests": "npm run test --mode=mainnet_tests",
    "test:mainnet-setup-core": "npm run test --mode=mainnet_setup_core",
    "test:mainnet-setup-endowments": "npm run test --mode=mainnet_setup_endowments",
    "test:mainnet-setup-giftcards": "npm run test --mode=mainnet_setup_giftcards",
    "test:mainnet-setup-junoswap": "npm run test --mode=mainnet_setup_junoswap",
    "test:mainnet-setup-halo": "npm run test --mode=mainnet_setup_halo",
    "test:mainnet-migrate-core": "npm run test --mode=mainnet_migrate_core",
    "test:mainnet-migrate-halo": "npm run test --mode=mainnet_migrate_halo",
    "lint": "eslint 'src/**/*.{ts,tsx}'",
    "format": "yarn run prettier --write 'src/**/*.{ts,tsx}'"
